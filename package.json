{
  "dependencies": {
    "@pixiv/three-vrm": "^0.2.0",
    "@types/three": "^0.103.2",
    "three": "^0.110.0",
    "typescript": "^3.7.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": ">=1",
    "lint-staged": ">=8",
    "prettier": "^1.19.1",
    "ts-loader": "^6.2.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "scripts": {
    "build": "webpack",
    "build:watch": "webpack --watch",
    "start:dev": "webpack-dev-server",
    "lint": "eslint src",
    "format": "prettier-eslint --write 'src/**/*.ts'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "yarn format",
      "yarn lint",
      "git add"
    ]
  }
}
